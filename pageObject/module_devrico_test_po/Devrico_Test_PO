var testloc = require('../../uiMap/uimap-devrico-test/devricoTestTaskLoc');

var DevricoTestFlyout = function () {
    var that = this;
    this.setTestData = element(testloc.set_testdata_Txtbx);
    this.spincoin = element(testloc.spin_coint_Btn);
    this.rowtwofirsttxt = element(testloc.middle_row_first_number_txt);
    this.rowtwosecondtxt = element(testloc.middle_row_second_number_txt);
    this.rowtwothirdtxt = element(testloc.middle_row_third_number_txt);
    this.rowtwofourthtxt = element(testloc.middle_row_fourth_number_txt);
    this.rowtwofifthtxt = element(testloc.middle_row_fifth_number_txt);
    this.wintxt = element(testloc.winbox_txt);



    this.setCointData1 = function () {
       var settestdata1 = '111';
       that.spincoin.sendKeys(settestdata1);
    };
    this.clickSpinCointBtn = function () {
            this.spincoin.click();
    };

    this.verifyResultFirstSet = function (tagstext1,tagstext2,tagstext3) {
    var txt = 'win';
    var losstxt = 'loss'
    var firsttxt = that.rowtwofirsttxt.get(tagstext1).getText();
    var secondtxt = that.rowsecondtxt.get(tagstext2).getText();
    var thirdtxt = that.rowtwothirdtxt.get(tagstext3).getText();
    if(expect(firsttxt).toEqual(1),expect(secondtxt.toEqual(1),expect(thirdtxt.toEqual(1))))
    console.log(txt)
    else
    console.log(losstxt)
    };

     this.verifyWinResultTxt = function (winresult) {
               var winresulttxt = that.wintxt.get(winresult).getText();
                if(expect(firsttxt).toEqual('Win 60 coins'))
                            console.log('displays correct text')
                            else
                            console.log('displays incorrect result')
                            };

    this.setCointData2 = function () {
           var settestdata2 = '1111';
           that.spincoin.sendKeys(settestdata2);
        };
    this.verifyResultSecondSet = function (tagstext1,tagstext2,tagstext3,tagstext4) {
        var txt = 'win';
        var losstxt = 'loss'
        var firsttxt = that.rowtwofirsttxt.get(tagstext1).getText();
        var secondtxt = that.rowsecondtxt.get(tagstext2).getText();
        var thirdtxt = that.rowtwothirdtxt.get(tagstext3).getText();
        var forthtxt = that.rowtwofourthtxt.get(tagstext4).getText();
        if(expect(firsttxt).toEqual(1),expect(secondtxt.toEqual(1),expect(thirdtxt.toEqual(1),expect(forthtxt.toEqual(1))))
        console.log(txt)
        else
        console.log(losstxt)
        };
         this.verifyWinResultTxt = function (winresult) {
                   var winresulttxt = that.wintxt.get(winresult).getText();
                    if(expect(firsttxt).toEqual('Win 80 coins'))
                                console.log('displays correct text')
                                else
                                console.log('displays incorrect result')
                                };

